import setlog.
import assert.
import log_h.

main =>
    setlog_eq1_1_test, setlog_eq1_2_test,
    setlog_eq2_1_test, 
    setlog_eq3_1_test, setlog_eq3_2_test, 
    setlog_eq4_1_test, 
    setlog_eq5_1_test, setlog_eq5_2_test, 
    setlog_eq6_1_test, 
    % 7
    setlog_eq8_1_test.

eq_tests() =>
    % check results...
    eq(1, 1, R),
    eq(1, Y, R),
    eq(X, 1, R),
    eq(X, Y, R),
    eq(1, 2, R).

neq_tests() =>
    % check results...
    neq(1, 2, R),
    neq(1, Y, R),
    neq(X, 1, R),
    neq(X, Y, R),
    neq(1, 1, R),
    neq(X, X, R).


% ---------------------------------------------------------
% *** eq1

setlog_eq1_1_test =>
        A = new_assert("setlog_eq1_1_test"),
        F = [$eq(1, 1)],
        solve(F, FF),
        A.assert_equal([], FF).

setlog_eq1_2_test ?=>
    A = new_assert("setlog_eq1_2_test"),
    F = [$eq(1, 2)],
    solve(F, FF),
    A.assert_fail_failed().
setlog_eq1_2_test =>
    A = new_assert("setlog_eq1_2_test"), 
    A.assert_fail().


